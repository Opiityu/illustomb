<div class="container">
  <div class="row">
    <div class="col">
      <h4 class="my-5"><%= @end_user.name %>さんの会員詳細</h4>
      <div class="d-flex align-items-center">
        <table class="table table-borderless">
          <tr>
            <th>会員ID</td>
            <td><%= @end_user.id %></td>
          </tr>
          <tr>
            <th>お名前</td>
            <td><%= @end_user.name %></td>
          </tr>
          <tr>
            <th>メールアドレス</th>
            <td><%= @end_user.email %></td>
          </tr>
          <tr>
    <th>投稿一覧</th>
    <td>
      <ul>
        <% @posts.each do |post| %>
          <%= image_tag post.image %>
        <% end %>
      </ul>
    </td>
  </tr>
        </table>
      </div>
    </div>
  </div>
</div>